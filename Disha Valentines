        }

        @keyframes sparkleExplode {
            0% {
                transform: translate(0, 0) scale(1);
                opacity: 1;
            }
            100% {
                transform: translate(var(--tx), var(--ty)) scale(0);
                opacity: 0;
            }
        }

        .sparkle-active {
            animation: sparkleExplode 1s ease-out forwards;
        }

        /* Falling Hearts */
        .falling-heart {
            position: fixed;
            font-size: 3em;
            pointer-events: none;
            z-index: 150;
        }

        @keyframes heartFall {
            to {
                transform: translateY(100vh) rotateZ(360deg);
                opacity: 0;
            }
        }

        .heart-fall-active {
            animation: heartFall 3s ease-in forwards;
        }

        /* Teddy Bear Bounce */
        .teddy-bounce {
            position: fixed;
