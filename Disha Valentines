  <script>
        let currentPage = 1;

        // Create floating background elements
        function createFloatingElements() {
            const bgContainer = document.getElementById('bgContainer');
            const elements = ['üåπ', '‚ù§Ô∏è', '‚ú®', '‚≠ê', 'üß∏', 'üå∏', 'üéÄ'];
            
            for (let i = 0; i < 40; i++) {
                const element = document.createElement('div');
                element.className = 'floating-element';
                element.textContent = elements[Math.floor(Math.random() * elements.length)];
                
                const leftPos = Math.random() * 100;
                const duration = 8 + Math.random() * 6;
                const delay = Math.random() * 5;
                const swayAmount = (Math.random() - 0.5) * 60;
                
                element.style.left = leftPos + '%';
                element.style.setProperty('--start-y', '0px');
                element.style.setProperty('--sway-amount', swayAmount + 'px');
                element.style.animation = `float ${duration}s linear ${delay}s infinite, sway ${3 + Math.random() * 2}s ease-in-out infinite`;
                
                bgContainer.appendChild(element);
            }
        }

        // Setup NO buttons to escape
        function setupNoButton(pageNum) {
            const noBtn = document.getElementById(`noBtn${pageNum}`);
            if (!noBtn) return;

            noBtn.addEventListener('mouseenter', () => {
                const randomX = (Math.random() - 0.5) * 400;
                const randomY = (Math.random() - 0.5) * 200;
                noBtn.style.transform = `translate(${randomX}px, ${randomY}px)`;
            });
        }

        // Navigate to question
        function goToQuestion(pageNum) {
            // Hide current page
            const pages = document.querySelectorAll('.page-wrapper');
            pages.forEach(page => page.classList.add('hidden'));

            // Show new page
            if (pageNum === 1) {
                document.getElementById('question1Page').classList.remove('hidden');
                setupNoButton(1);
            } else if (pageNum === 2) {
                document.getElementById('question2Page').classList.remove('hidden');
                createRosePetals();
                setupNoButton(2);
            } else if (pageNum === 3) {
                document.getElementById('question3Page').classList.remove('hidden');
                setupNoButton(3);
            }
            currentPage = pageNum;
        }

        // Handle YES button
        function handleYes(pageNum) {
            if (pageNum === 1) {
                createPopHearts();
                setTimeout(() => goToQuestion(2), 800);
            } else if (pageNum === 2) {
                createPopHearts();
                setTimeout(() => goToQuestion(3), 800);
            } else if (pageNum === 3) {
                celebrateAndWin();
            }
        }

        // Pop hearts animation
        function createPopHearts() {
            const container = document.querySelector('.button-container');
            const rect = container.getBoundingClientRect();
            
            for (let i = 0; i < 12; i++) {
                const heart = document.createElement('div');
                heart.className = 'pop-heart';
                heart.textContent = 'üíñ';
                heart.style.left = (rect.left + rect.width / 2) + 'px';
                heart.style.top = (rect.top + rect.height / 2) + 'px';
                heart.style.animation = 'popAndFade 0.6s ease-out forwards';
                
                document.body.appendChild(heart);
                setTimeout(() => heart.remove(), 600);
            }
        }

        // Rose petals falling
        function createRosePetals() {
            for (let i = 0; i < 20; i++) {
                setTimeout(() => {
                    const petal = document.createElement('div');
                    petal.className = 'petal';
                    petal.textContent = 'üåπ';
                    petal.style.left = Math.random() * 100 + '%';
                    petal.style.top = '-50px';
                    petal.style.fontSize = (0.8 + Math.random() * 0.5) + 'em';
                    petal.style.animation = `petalFall ${3 + Math.random() * 2}s ease-in forwards`;
                    
                    document.body.appendChild(petal);
                    setTimeout(() => petal.remove(), 5000);
                }, i * 100);
            }
        }

        // Celebration effects
        function createConfetti() {
            const colors = ['#ff1493', '#ff69b4', '#ffb3d9', '#ffc0cb', '#d946a6', '#c084fc'];
            for (let i = 0; i < 100; i++) {
                const confetti = document.createElement('div');
                confetti.className = 'confetti';
                confetti.style.left = Math.random() * 100 + '%';
                confetti.style.top = '-10px';
                confetti.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                confetti.style.animation = `confettiFall ${2 + Math.random() * 1}s ease-in forwards`;
                document.body.appendChild(confetti);
                
                setTimeout(() => confetti.remove(), 3000);
            }
        }

        function createSparkles() {
            const center = { x: window.innerWidth / 2, y: window.innerHeight / 2 };
            for (let i = 0; i < 25; i++) {
                const sparkle = document.createElement('div');
                sparkle.className = 'sparkle-burst';
                sparkle.textContent = '‚ú®';
                sparkle.style.left = center.x + 'px';
                sparkle.style.top = center.y + 'px';
                
                const angle = (i / 25) * Math.PI * 2;
                const distance = 250;
                const tx = Math.cos(angle) * distance;
                const ty = Math.sin(angle) * distance;
                
                sparkle.style.setProperty('--tx', tx + 'px');
                sparkle.style.setProperty('--ty', ty + 'px');
                sparkle.style.animation = 'sparkleExplode 1s ease-out forwards';
                
                document.body.appendChild(sparkle);
                setTimeout(() => sparkle.remove(), 1000);
            }
        }

        function createFallingHearts() {
            for (let i = 0; i < 20; i++) {
                setTimeout(() => {
                    const heart = document.createElement('div');
                    heart.className = 'falling-heart';
                    heart.textContent = 'üíñ';
                    heart.style.left = Math.random() * 100 + '%';
                    heart.style.top = '-50px';
                    heart.style.animation = 'heartFall 3s ease-in forwards';
                    
                    document.body.appendChild(heart);
                    setTimeout(() => heart.remove(), 3000);
                }, i * 100);
            }
        }

        function createTeddyBounces() {
            for (let i = 0; i < 8; i++) {
                setTimeout(() => {
                    const teddy = document.createElement('div');
                    teddy.className = 'teddy-bounce';
                    teddy.textContent = 'üß∏';
                    teddy.style.left = (Math.random() * 60 + 20) + '%';
                    teddy.style.top = (Math.random() * 40 + 40) + '%';
                    teddy.style.animation = `teddyBounce 0.6s ease-in-out`;
                    
                    document.body.appendChild(teddy);
                    setTimeout(() => teddy.remove(), 600);
                }, i * 150);
            }
        }

        function createBalloons() {
            const colors = ['#ff1493', '#ff69b4', '#ffb3d9', '#ffc0cb', '#d946a6', '#c084fc'];
            for (let i = 0; i < 12; i++) {
                const balloon = document.createElement('div');
                balloon.className = 'balloon';
                balloon.style.left = Math.random() * 100 + '%';
                balloon.style.bottom = '-60px';
                balloon.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                balloon.style.animation = `rise ${3 + Math.random() * 1}s ease-in forwards`;
                balloon.style.animationDelay = (i * 0.15) + 's';
                
                document.body.appendChild(balloon);
                setTimeout(() => balloon.remove(), 4500);
            }
        }

        function celebrateAndWin() {
            // Hide all pages
            document.querySelectorAll('.page-wrapper').forEach(page => page.classList.add('hidden'));
            
            // Show celebration page
            document.getElementById('celebrationPage').classList.remove('hidden');
            
            // Trigger all celebration effects
            createConfetti();
            createSparkles();
            createFallingHearts();
            createTeddyBounces();
            createBalloons();
            
            // Create rose petals
            for (let i = 0; i < 30; i++) {
                setTimeout(() => {
                    const petal = document.createElement('div');
                    petal.className = 'petal';
                    petal.textContent = 'üåπ';
                    petal.style.left = Math.random() * 100 + '%';
                    petal.style.top = '-50px';
                    petal.style.fontSize = (0.8 + Math.random() * 0.5) + 'em';
                    petal.style.animation = `petalFall ${3 + Math.random() * 2}s ease-in forwards`;
                    
                    document.body.appendChild(petal);
                    setTimeout(() => petal.remove(), 5000);
                }, i * 80);
            }
        }

        // Initialize
        createFloatingElements();
        setupNoButton(1);
    </script>
