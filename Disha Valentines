        @keyframes teddyBounce {
            0%, 100% {
                transform: translateY(0) scaleY(1);
            }
            50% {
                transform: translateY(-60px) scaleY(0.95);
            }
        }

        .teddy-active {
            animation: teddyBounce 0.6s ease-in-out;
        }

        @keyframes fadeInTyping {
            0% { text-shadow: 0 0 20px rgba(217, 70, 166, 0.6); }
            50% { text-shadow: 0 0 40px rgba(217, 70, 166, 0.8); }
